set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set complex_mass_scheme False
import model SiM_SVD_UFO
define p = g u c d s u~ c~ d~ s~
define j = g u c d s u~ c~ d~ s~
define l+ = e+ mu+
define l- = e- mu-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
define W = w+ w-
generate p p > W chi chi~, W > j j
add process p p > Z chi chi~, Z > j j
output SiM_SVD_monoWZ_8TeV -f
